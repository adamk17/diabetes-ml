# S3 (real values if you want to pull the model)
AWS_ACCESS_KEY_ID=your-real-key
AWS_SECRET_ACCESS_KEY=your-real-secret
AWS_REGION=eu-central-1
MODEL_BUCKET=diabetes-ml-bucket
MODEL_PREFIX=tf_model
USE_IAM_AUTH=false

# Local PostgreSQL DB (from docker-compose)
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=diabetes_db
DB_POOL_MIN=1
DB_POOL_MAX=10
DB_IAM_AUTH=false
DB_SSL_MODE=disable
DB_CONNECT_TIMEOUT=5

# CloudWatch disabled locally
ENABLE_CLOUDWATCH=false
CLOUDWATCH_LOG_GROUP=model-prediction-api

# API version
API_VERSION=1.0.0
